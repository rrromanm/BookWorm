<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="576.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sep.view.EventsViewController">
    <children>
        <MenuBar layoutY="2.0" prefHeight="9.0" prefWidth="1024.0">
            <menus>
                <Menu mnemonicParsing="false" text="Events">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Close" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <TableView fx:id="eventTableView" layoutX="14.0" layoutY="37.0" prefHeight="490.0" prefWidth="1002.0">
            <columns>
                <TableColumn fx:id="titleColumn" prefWidth="277.0" text="Title" />
                <TableColumn fx:id="descriptionColumn" prefWidth="557.0" text="Description" />
                <TableColumn fx:id="dateColumn" minWidth="0.0" prefWidth="167.0" text="Date" />
            </columns>
        </TableView>
        <Button fx:id="back" layoutX="14.0" layoutY="537.0" mnemonicParsing="false" onAction="#backButtonClicked" text="Back" />
    </children>
</AnchorPane>
